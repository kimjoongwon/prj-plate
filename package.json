{
  "name": "core",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@10.16.0",
  "scripts": {
    "build": "turbo build",
    "build:server": "turbo build --filter=server",
    "build:admin": "turbo build --filter=admin",
    "build:mobile": "turbo build --filter=mobile",
    "build:storybook": "turbo build --filter=storybook",
    "build:packages": "turbo build --filter='./packages/**'",
    "build:pkg": "turbo build --filter",
    "clean": "turbo clean",
    "start:dev": "turbo start:dev",
    "start:mobile:ios": "turbo start:dev --filter=mobile",
    "start:mobile:aos": "turbo start:dev --filter=mobile",
    "start:server": "turbo start:dev --filter=@cocrepo/schema --filter=server",
    "start:admin": "turbo start:dev --filter=admin",
    "start:storybook": "turbo start:dev --filter=storybook",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "turbo format",
    "test": "turbo test",
    "storybook": "pnpm -C apps/mobile storybook",
    "version:patch": "pnpm --filter './packages/**' --parallel exec npm version patch --no-git-tag-version",
    "version:minor": "pnpm --filter './packages/**' --parallel exec npm version minor --no-git-tag-version",
    "version:major": "pnpm --filter './packages/**' --parallel exec npm version major --no-git-tag-version",
    "version:pkg": "node scripts/version-pkg.js",
    "publish:packages": "pnpm --filter './packages/**' --parallel publish --access public --no-git-checks",
    "publish:dry": "pnpm --filter './packages/**' --parallel publish --access public --no-git-checks --dry-run",
    "publish:pkg": "pnpm --filter",
    "publish:pkg:dry": "pnpm --filter",
    "update:app-deps": "node scripts/update-app-deps.js",
    "release:patch": "pnpm version:patch && pnpm build:packages && pnpm publish:packages && pnpm update:app-deps",
    "release:minor": "pnpm version:minor && pnpm build:packages && pnpm publish:packages && pnpm update:app-deps",
    "release:major": "pnpm version:major && pnpm build:packages && pnpm publish:packages && pnpm update:app-deps",
    "release:pkg": "node scripts/release-pkg.js",
    "release:pkg:dry": "node scripts/release-pkg.js"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "vite": "catalog:",
    "@vitejs/plugin-react-swc": "catalog:",
    "vitest": "catalog:",
    "@vitest/ui": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "typescript": "catalog:",
    "@types/node": "catalog:"
  }
}
