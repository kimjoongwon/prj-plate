{
  "name": "@shared/api-client",
  "version": "1.0.0",
  "description": "Generated API client and types from OpenAPI specs",
  "keywords": [
    "api-client",
    "openapi",
    "react-query",
    "typescript",
    "axios"
  ],
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:prod": "NODE_ENV=production tsup",
    "dev": "tsup --watch",
    "start:dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "clean:dist": "rm -rf dist",
    "codegen:dev": "orval --config orval.dev.config.js",
    "codegen:stg": "orval --config orval.stg.config.js",
    "codegen:prod": "orval --config orval.prod.config.js",
    "codegen": "npm run codegen:dev",
    "prebuild": "npm run clean:dist",
    "prepack": "npm run build:prod"
  },
  "dependencies": {
    "@tanstack/react-query": "5.83.0",
    "axios": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "orval": "^7.8.0",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "author": "kimjoongwon",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/prj-core.git",
    "directory": "packages/shared-api-client"
  },
  "bugs": {
    "url": "https://github.com/your-org/prj-core/issues"
  },
  "homepage": "https://github.com/your-org/prj-core/tree/main/packages/shared-api-client#readme"
}
