{
  "name": "@shared/api-client",
  "version": "1.0.0",
  "description": "Generated API client and types from OpenAPI specs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "start:dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "codegen:dev": "orval --config orval.dev.config.js",
    "codegen:stg": "orval --config orval.stg.config.js",
    "codegen:prod": "orval --config orval.prod.config.js",
    "codegen": "npm run codegen:dev"
  },
  "dependencies": {
    "@tanstack/react-query": "5.66.9",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/node": "^20",
    "orval": "^7.8.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "author": "kimjoongwon",
  "license": "ISC"
}
