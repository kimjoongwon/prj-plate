    "framer-motion": "^10.16.4",
    "lodash-es": "4.17.21",
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.5",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.11.0",
    "tailwind-variants": "0.1.14",
    "uuid": "^9.0.1",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.9",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "eslint": "8.50.0",
    "eslint-config-custom": "workspace:*",
    "postcss": "8.4.31",
    "tailwindcss": "^3.3.3",
