{
  "name": "@coc/ui",
  "version": "1.2.5",
  "description": "ui library",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "type": "module",
  "scripts": {
    "build": "tsup --config tsup.config.ts --turbo",
