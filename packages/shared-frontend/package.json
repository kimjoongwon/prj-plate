{
  "name": "@shared/frontend",
  "version": "1.2.5",
  "description": "ui library",
  "sideEffects": [
    "**/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "build": "tsup --config tsup.config.ts && pnpm run copy-css",
    "start:dev": "tsup --watch && pnpm run copy-css",
    "copy-css": "copyfiles --all \"./globals.css\" dist",
    "lint": "tsc --noEmit && eslint \"src/*.ts*\"",
    "test": "vitest",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "codegen": "openapi-rq -i http://localhost:3005/api-json --output ./src/api -c axios"
  },
  "author": "kimjoongwon",
  "license": "ISC",
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^43.2.0",
    "@ckeditor/ckeditor5-react": "^9.3.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@internationalized/date": "^3.5.6",
    "@mui/material": "^6.1.2",
    "@nextui-org/react": "2.4.8",
    "@nozbe/watermelondb": "^0.27.1",
    "@tanstack/react-query": "5.59.0",
    "@tanstack/react-table": "^8.20.5",
    "ajv": "^8.17.1",
    "axios": "^1.7.7",
    "ckeditor5": "^43.2.0",
    "dayjs": "1.11.13",
    "effect": "^3.8.4",
    "framer-motion": "^11.11.1",
    "lodash-es": "4.17.21",
    "mobx": "^6.13.3",
    "mobx-react-lite": "^4.0.7",
    "next": "14.2.14",
    "path-parser": "6.1.0",
    "react": "18.3.1",
    "react-calendar": "^5.0.0",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "react-quill": "2.0.0",
    "react-toastify": "^10.0.5",
    "remeda": "^2.14.0",
    "tailwind-variants": "0.2.1",
    "tailwindcss": "^3.4.13",
    "ts-pattern": "^5.4.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@tanstack/react-query-devtools": "^5.59.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.2",
    "copyfiles": "^2.4.1",
    "eslint": "9.12.0",
    "jsdom": "^25.0.1",
    "msw": "^2.4.9",
    "orval": "^7.1.1",
    "react-test-renderer": "^18.3.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "2.1.2"
  }
}
