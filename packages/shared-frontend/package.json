{
  "name": "@shared/frontend",
  "version": "1.2.5",
  "description": "ui library",
  "sideEffects": [
    "**/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "build": "tsup --config tsup.config.ts && pnpm run copy-css",
    "start:dev": "tsup --watch && pnpm run copy-css",
    "copy-css": "copyfiles --all \"./globals.css\" dist",
    "lint": "tsc --noEmit && eslint \"src/*.ts*\"",
    "test": "vitest",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "codegen": "openapi-rq -i http://localhost:3005/api-json --output ./src/api -c axios"
  },
  "author": "kimjoongwon",
  "license": "ISC",
  "dependencies": {
    "@nextui-org/react": "2.4.6",
    "@nozbe/watermelondb": "^0.27.1",
    "@tanstack/react-query": "5.51.11",
    "@tanstack/react-table": "^8.19.3",
    "@vanilla-extract/css": "^1.15.3",
    "ajv": "8",
    "axios": "^1.7.2",
    "dayjs": "1.11.12",
    "framer-motion": "^11.3.17",
    "lodash-es": "4.17.21",
    "mobx": "^6.13.1",
    "mobx-react-lite": "^4.0.7",
    "next": "14.2.5",
    "openapi-types": "^12.1.3",
    "path-parser": "6.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "react-quill": "2.0.0",
    "react-toastify": "^10.0.5",
    "tailwind-variants": "0.2.1",
    "tailwindcss": "^3.4.6",
    "uuid": "^10.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "^1.4.1",
    "@faker-js/faker": "^8.4.1",
    "@tanstack/react-query-devtools": "^5.51.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "copyfiles": "^2.4.1",
    "eslint": "9.7.0",
    "jsdom": "^24.1.1",
    "msw": "^2.3.4",
    "orval": "^6.31.0",
    "tsup": "^8.2.2",
    "typescript": "^5.5.4",
    "vitest": "2.0.4"
  }
}
