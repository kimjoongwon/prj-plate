{
  "name": "@shared/frontend",
  "version": "1.2.5",
  "description": "ui library",
  "sideEffects": [
    "**/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "build": "tsup --config tsup.config.ts && pnpm run copy-css",
    "start:dev": "tsup --watch && pnpm run copy-css",
    "copy-css": "copyfiles --all \"./globals.css\" dist",
    "lint": "tsc --noEmit && eslint \"src/*.ts*\"",
    "test": "vitest",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "author": "kimjoongwon",
  "license": "ISC",
  "dependencies": {
    "@tanstack/react-query": "5.24.1",
    "@tanstack/react-table": "^8.10.7",
    "axios": "^1.6.7",
    "dayjs": "1.11.10",
    "framer-motion": "^10.16.5",
    "lodash-es": "4.17.21",
    "mobx": "^6.11.0",
    "mobx-react-lite": "^4.0.5",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.12.0",
    "react-quill": "2.0.0",
    "react-toastify": "^9.1.3",
    "tailwind-variants": "0.1.18",
    "uuid": "^9.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/lodash-es": "^4.17.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "copyfiles": "^2.4.1",
    "eslint": "8.54.0",
    "eslint-config-custom": "workspace:*",
    "msw": "^2.2.10",
    "orval": "^6.25.0",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "0.34.6"
  }
}
