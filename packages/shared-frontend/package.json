{
  "name": "@shared/frontend",
  "version": "1.2.5",
  "description": "ui library",
  "sideEffects": [
    "**/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "build": "tsup --config tsup.config.ts && pnpm run copy-css",
    "start:dev": "tsup --watch && pnpm run copy-css",
    "copy-css": "copyfiles --all \"./globals.css\" dist",
    "lint": "tsc --noEmit && eslint \"src/*.ts*\"",
    "test": "vitest",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "author": "kimjoongwon",
  "license": "ISC",
  "dependencies": {
    "@tanstack/react-query": "5.28.6",
    "@tanstack/react-table": "^8.15.0",
    "axios": "^1.6.8",
    "dayjs": "1.11.10",
    "framer-motion": "^11.0.20",
    "lodash-es": "4.17.21",
    "mobx": "^6.12.1",
    "mobx-react-lite": "^4.0.6",
    "next": "14.1.4",
    "openapi-types": "^12.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "react-quill": "2.0.0",
    "react-toastify": "^10.0.5",
    "tailwind-variants": "0.2.1",
    "tailwindcss": "^3.4.1",
    "uuid": "^9.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "copyfiles": "^2.4.1",
    "eslint": "8.57.0",
    "eslint-config-custom": "workspace:*",
    "msw": "^2.2.10",
    "orval": "^6.25.0",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vitest": "1.4.0"
  }
}
