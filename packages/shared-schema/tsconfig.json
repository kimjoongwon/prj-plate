{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "paths": {
      "@prisma/client": ["./generated/client/index.js"],
      "@cocrepo/schema/client": ["./generated/client/index.js"],
      "@cocrepo/schema/client/runtime/library": [
        "./generated/client/runtime/library.js"
      ]
    },
    "outDir": "./dist",
    "incremental": false,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false
  },
  "exclude": ["generated", "node_modules", "dist"]
}
