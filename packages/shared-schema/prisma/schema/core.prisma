model Category {
  id                   String                @id @default(uuid())
  seq                  Int                   @unique @default(autoincrement())
  createdAt            DateTime              @default(now()) @db.Timestamptz(6)
  updatedAt            DateTime?             @updatedAt @db.Timestamptz(6)
  removedAt            DateTime?             @db.Timestamptz(6)
  name                 String                @unique
  type                 CategoryTypes         @default(User)
  parentId             String?
  tenantId             String
  parent               Category?             @relation("CategoryToCategory", fields: [parentId], references: [id])
  children             Category[]            @relation("CategoryToCategory")
  tenant               Tenant                @relation(fields: [tenantId], references: [id])
  roleClassifications  RoleClassification[]
  spaceClassifications SpaceClassification[]
  fileClassifications  FileClassification[]
  userClassifications  UserClassification[]
}

model Group {
  id                String             @id @default(uuid())
  seq               Int                @unique @default(autoincrement())
  createdAt         DateTime           @default(now()) @db.Timestamptz(6)
  updatedAt         DateTime?          @updatedAt @db.Timestamptz(6)
  removedAt         DateTime?          @db.Timestamptz(6)
  name              String
  type              GroupTypes         @default(User)
  label             String?
  tenantId          String
  tenant            Tenant             @relation(fields: [tenantId], references: [id])
  userAssociations  UserAssociation[]
  roleAssociations  RoleAssociation[]
  spaceAssociations SpaceAssociation[]
  fileAssociations  FileAssociation[]
}

model Exercise {
  id          String    @id @default(uuid())
  seq         Int       @unique @default(autoincrement())
  createdAt   DateTime  @default(now())
  updatedAt   DateTime? @updatedAt @db.Timestamptz(6)
  removedAt   DateTime? @db.Timestamptz(6)
  duration    Int
  count       Int
  taskId      String    @unique
  description String?
  imageFileId String?
  name        String
  videoFileId String?
  task        Task      @relation(fields: [taskId], references: [id])
}

model Service {
  id        String       @id @default(uuid())
  seq       Int          @unique @default(autoincrement())
  createdAt DateTime     @default(now()) @db.Timestamptz(6)
  updatedAt DateTime?    @updatedAt @db.Timestamptz(6)
  removedAt DateTime?    @db.Timestamptz(6)
  label     String
  name      ServiceNames @unique
}

model Profile {
  id           String    @id @default(uuid())
  seq          Int       @unique @default(autoincrement())
  createdAt    DateTime  @default(now())
  updatedAt    DateTime? @updatedAt @db.Timestamptz(6)
  removedAt    DateTime? @db.Timestamptz(6)
  name         String
  nickname     String    @unique
  userId       String
  avatarFileId String?
  user         User      @relation(fields: [userId], references: [id])
}

model Tenant {
  id          String       @id @default(uuid())
  seq         Int          @unique @default(autoincrement())
  createdAt   DateTime     @default(now())
  updatedAt   DateTime?    @updatedAt @db.Timestamptz(6)
  removedAt   DateTime?    @db.Timestamptz(6)
  userId      String
  spaceId     String
  roleId      String
  main        Boolean
  assignments Assignment[]
  categories  Category[]
  contents    Content[]
  files       File[]
  groups      Group[]
  role        Role         @relation(fields: [roleId], references: [id])
  space       Space        @relation(fields: [spaceId], references: [id])
  user        User         @relation(fields: [userId], references: [id])
  timelines   Timeline[]
}

model Timeline {
  id        String    @id @default(uuid())
  seq       Int       @unique @default(autoincrement())
  createdAt DateTime  @default(now()) @db.Timestamptz(6)
  updatedAt DateTime? @updatedAt @db.Timestamptz(6)
  removedAt DateTime? @db.Timestamptz(6)
  tenantId  String
  sessions  Session[]
  tenant    Tenant    @relation(fields: [tenantId], references: [id])
}

model Session {
  id                 String              @id @default(uuid())
  seq                Int                 @unique @default(autoincrement())
  createdAt          DateTime            @default(now()) @db.Timestamptz(6)
  updatedAt          DateTime?           @updatedAt @db.Timestamptz(6)
  removedAt          DateTime?           @db.Timestamptz(6)
  type               SessionTypes        @default(ONE_TIME)
  repeatCycleType    RepeatCycleTypes?
  startDateTime      DateTime?           @db.Timestamptz(6)
  endDateTime        DateTime?           @db.Timestamptz(6)
  recurringDayOfWeek RecurringDayOfWeek?
  recurringMonth     Int?
  timelineId         String
  label              String
  name               String
  program            Program?
  timeline           Timeline            @relation(fields: [timelineId], references: [id])
}

model Program {
  id           String    @id @default(uuid())
  seq          Int       @unique @default(autoincrement())
  createdAt    DateTime  @default(now()) @db.Timestamptz(6)
  updatedAt    DateTime? @updatedAt @db.Timestamptz(6)
  removedAt    DateTime? @db.Timestamptz(6)
  routineId    String    @unique
  sessionId    String    @unique
  instructorId String
  capacity     Int
  routine      Routine   @relation(fields: [routineId], references: [id])
  session      Session   @relation(fields: [sessionId], references: [id])
}

model Activity {
  id        String    @id @default(uuid())
  seq       Int       @unique @default(autoincrement())
  createdAt DateTime  @default(now()) @db.Timestamptz(6)
  updatedAt DateTime? @updatedAt @db.Timestamptz(6)
  removedAt DateTime? @db.Timestamptz(6)
  routineId String
  taskId    String

  @@unique([routineId, taskId])
}

model Routine {
  id        String    @id @default(uuid())
  seq       Int       @unique @default(autoincrement())
  createdAt DateTime  @default(now()) @db.Timestamptz(6)
  updatedAt DateTime? @updatedAt @db.Timestamptz(6)
  removedAt DateTime? @db.Timestamptz(6)
  name      String
  label     String
  program   Program?
}

model Task {
  id        String    @id @default(uuid())
  seq       Int       @unique @default(autoincrement())
  createdAt DateTime  @default(now()) @db.Timestamptz(6)
  updatedAt DateTime? @updatedAt @db.Timestamptz(6)
  removedAt DateTime? @db.Timestamptz(6)
  tenantId  String
  exercise  Exercise?
}

model Assignment {
  id        String    @id @default(uuid())
  seq       Int       @unique @default(autoincrement())
  createdAt DateTime  @default(now())
  updatedAt DateTime? @updatedAt @db.Timestamptz(6)
  removedAt DateTime? @db.Timestamptz(6)
  roleId    String
  tenantId  String
  role      Role      @relation(fields: [roleId], references: [id])
  tenant    Tenant    @relation(fields: [tenantId], references: [id])
}

model Action {
  id         String         @id @default(uuid())
  seq        Int            @unique @default(autoincrement())
  createdAt  DateTime       @default(now())
  updatedAt  DateTime?      @updatedAt @db.Timestamptz(6)
  removedAt  DateTime?      @db.Timestamptz(6)
  name       AbilityActions @default(CREATE)
  conditions Json?
  tenantId   String
}

model Subject {
  id        String    @id @default(uuid())
  seq       Int       @unique @default(autoincrement())
  createdAt DateTime  @default(now())
  updatedAt DateTime? @updatedAt @db.Timestamptz(6)
  removedAt DateTime? @db.Timestamptz(6)
  name      String    @unique
  tenantId  String
}

model Ability {
  id          String       @id @default(uuid())
  seq         Int          @unique @default(autoincrement())
  createdAt   DateTime     @default(now()) @db.Timestamptz(6)
  updatedAt   DateTime?    @updatedAt @db.Timestamptz(6)
  removedAt   DateTime?    @db.Timestamptz(6)
  type        AbilityTypes
  roleId      String
  description String?
  conditions  Json?
  subjectId   String
  tenantId    String
  role        Role         @relation(fields: [roleId], references: [id])
}

model Post {
  id        String    @id @default(uuid())
  seq       Int       @unique @default(autoincrement())
  createdAt DateTime  @default(now()) @db.Timestamptz(6)
  updatedAt DateTime? @updatedAt @db.Timestamptz(6)
  removedAt DateTime? @db.Timestamptz(6)
  contentId String
  content   Content   @relation(fields: [contentId], references: [id])
}

model Content {
  id          String    @id @default(uuid())
  seq         Int       @unique @default(autoincrement())
  createdAt   DateTime  @default(now()) @db.Timestamptz(6)
  updatedAt   DateTime? @updatedAt @db.Timestamptz(6)
  removedAt   DateTime? @db.Timestamptz(6)
  title       String?
  description String?
  type        TextTypes @default(Editor)
  text        String?   @db.VarChar(1000)
  fileId      String?
  tenantId    String
  tenant      Tenant    @relation(fields: [tenantId], references: [id])
  posts       Post[]
}

enum SMSStatus {
  PROGRESS
  PENDING
  SENT
  FAILED
}

enum ReservationStatus {
  PENDING
  CONFIRMED
  CANCELLED
}

enum SessionTypes {
  ONE_TIME
  ONE_TIME_RANGE
  RECURRING
}

enum RepeatCycleTypes {
  WEEKLY
  MONTHLY
}

enum SessionEndTypes {
  NEVER
  ON_DATE
  AFTER_OCCURRENCES
}

enum RecurringDayOfWeek {
  MONDAY
  TUESDAY
  WEDNESDAY
  THURSDAY
  FRIDAY
  SATURDAY
  SUNDAY
}

enum AbilityActions {
  CREATE
  READ
  UPDATE
  DELETE
  ACCESS
}

enum AbilityTypes {
  CAN
  CAN_NOT
}

enum Roles {
  USER
  SUPER_ADMIN
  ADMIN
}

enum TemplateNames {
  WELCOME
  EMAIL_VERIFICATION
  RESET_PASSWORD
}

enum QnaStatus {
  OPEN
  CLOSED
}

enum TextTypes {
  Editor
  Input
  Textarea
}

enum CategoryTypes {
  Role
  Space
  File
  User
}

enum GroupTypes {
  Role
  Space
  File
  User
}

enum ServiceNames {
  user
  role
  space
  timeline
  file
  task
  program
}
