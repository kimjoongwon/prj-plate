{
  "name": "@cocrepo/hook",
  "version": "1.3.7",
  "description": "Shared React hook library",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts",
      "default": "./index.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "files": [
    "src",
    "index.ts",
    "README.md"
  ],
  "scripts": {
    "build": "echo 'Build not needed - using source files directly'",
    "start:dev": "echo 'Development mode - no build needed'",
    "lint": "echo 'Linting skipped for now'",
    "lint:fix": "echo 'Linting skipped for now'",
    "format": "biome format --write .",
    "type-check": "echo 'Type checking skipped due to memory constraints'",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "clean:dist": "echo 'No dist folder to clean'"
  },
  "author": "kimjoongwon",
  "license": "ISC",
  "devDependencies": {
    "react": "catalog:",
    "mobx": "catalog:",
    "mobx-react-lite": "catalog:",
    "@types/react": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "@cocrepo/type": "workspace:*",
    "@cocrepo/toolkit": "workspace:*"
  }
}
