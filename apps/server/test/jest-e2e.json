{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/test/(.*)": "<rootDir>/../libs/test/src/$1",
    "@app/test": "<rootDir>/../libs/test/src",
    "@app/wally/(.*)": "<rootDir>/../libs/wally/src/$1",
    "@app/wally": "<rootDir>/../libs/wally/src"
  }
}