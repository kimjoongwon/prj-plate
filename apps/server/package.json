    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.0.2",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.4.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^4.14.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "canvas": "^2.11.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.22.2",
    "nestjs-prisma": "^0.20.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfjs-dist": "^3.7.107",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.59",
    "@types/express": "^4.17.13",
    "@types/node": "18.11.18",
