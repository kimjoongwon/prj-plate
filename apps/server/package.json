    "nodemon": "^2.0.22",
    "prettier": "^2.3.2",
    "prisma": "^4.14.1",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
