{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "type-check": "tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "cd ../packages/shared-prisma && pnpm run generate",
    "db:push": "cd ../packages/shared-prisma && pnpm run db:push",
    "db:prod:generate": "cd ../packages/shared-prisma && dotenv -e .env.prod pnpm run generate",
    "clean": "rm -rf dist && rm -rf node_modules"
  },
  "prisma": {
    "seed": "cd ../packages/shared-prisma && ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
