{
  "name": "admin",
  "private": true,
  "scripts": {
    "build": "next build",
    "start:dev": "next dev -p 3004 --turbo",
    "lint": "tsc --noEmit && eslint \"app/**/*.ts*\"",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "start": "next start",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "codegen": "openapi-rq -i http://localhost:3005/api-json --output ./src/api -c axios"
  },
  "dependencies": {
    "@apollo/client": "3.8.7",
    "@apollo/experimental-nextjs-app-support": "0.5.1",
    "@coc/shared": "workspace:*",
    "@nextui-org/react": "2.2.9",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tanstack/react-query": "5.24.1",
    "@tanstack/react-table": "^8.10.7",
    "ajv": "6",
    "axios": "^1.6.7",
    "cookies-next": "4.1.0",
    "dayjs": "1.11.10",
    "form-data": "4.0.0",
    "framer-motion": "^10.16.5",
    "graphql": "^16.8.1",
    "graphql-ws": "5.14.2",
    "json-schema-to-zod": "2.0.14",
    "jsonwebtoken": "9.0.2",
    "lodash-es": "4.17.21",
    "mobx": "^6.11.0",
    "mobx-react-lite": "^4.0.5",
    "next": "14.0.3",
    "path-parser": "6.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "react-icons": "4.12.0",
    "react-quill": "2.0.0",
    "react-toastify": "^9.1.3",
    "server-only": "0.0.1",
    "tailwind-variants": "0.1.18",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "0.5.3",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@next/bundle-analyzer": "14.0.4",
    "@openapi-codegen/cli": "2.0.0",
    "@openapi-codegen/typescript": "8.0.0",
    "@parcel/watcher": "2.3.0",
    "@types/file-saver": "2.0.7",
    "@types/jsonwebtoken": "9.0.5",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "10.4.16",
    "eslint-config-custom": "workspace:*",
    "postcss": "8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "5.3.3"
  }
