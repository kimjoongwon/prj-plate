{
  "name": "admin",
  "private": true,
  "scripts": {
    "build": "next build",
    "start:dev": "next dev -p 3004",
    "lint": "tsc --noEmit && eslint \"app/**/*.ts*\"",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "start": "next start",
    "test": "vitest --watch",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "codegen": "openapi-rq -i http://localhost:3005/api-json --output ./src/api -c axios"
  },
